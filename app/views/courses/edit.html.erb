<%= form_for  @course do |f| %>
   <%= f.label :name  %>
  <%= f.text_field :name, class:"names" value: @course.name%>
  <%= f.label :description %>
  <%= f.text_field :description, class:"names" value: @course.description %>
  <%= f.submit %>
<% end %>
<%= link_to 'Delete', course_path(@course), method: :delete %>