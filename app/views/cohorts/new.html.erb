<%= form_for  @cohort do |f| %>
   <%= f.label :start_date  %>
  <%= f.text_field :start_date, class:"names" %>
  <%= f.label :end_date %>
  <%= f.text_field :end_date, class:"names" %>
  <%= f.label :course_id %>
  <%= f.text_field :course_id, class:"course_id" %>
  <%= f.label :teacher_id %>
  <%= f.text_field :teacher_id, class:"teacher_id" %>
  <%= f.submit %>
<% end %>
